  <script>

    /***************** Waypoints ******************/

    $(document).ready(function() {

      $('body').waypoint(function(direction) {
        if (direction == 'down') {
          $('.Mynavbar').addClass('sticky');
        } else {
          $('.Mynavbar').removeClass('sticky');
        }
      }, {
        offset: '-10'
      });

      $('.wp1').waypoint(function() {
        $('.wp1').addClass('animated fadeInLeft');
        $('.wp1a').addClass('animated fadeInRight');
      }, {
        offset: '75%'
      });
      $('.wp2').waypoint(function() {
        $('.wp2a').addClass('animated fadeInUp');
      }, {
        offset: '75%'
      });
      $('.wp3').waypoint(function() {
        $('.wp3').addClass('animated fadeInLeft');
        $('.wp3a').addClass('animated fadeInRight');
      }, {
        offset: '55%'
      });
      $('.wp4').waypoint(function() {
        $('.wp4a').addClass('animated fadeInUp');
      }, {
        offset: '55%'
      });
      $('.wp5').waypoint(function() {
        $('.wp5').addClass('animated fadeInLeft');
        $('.wp5a').addClass('animated fadeInRight');
      }, {
        offset: '55%'
      });
      $('.wp6').waypoint(function() {
        $('.wp6a').addClass('animated fadeInUp');
      }, {
        offset: '55%'
      });
      $('.wp7').waypoint(function() {
        $('.wp7').addClass('animated fadeInLeft');
        $('.wp7a').addClass('animated fadeInRight');
      }, {
        offset: '55%'
      });



      // side bar script

      function toggleSlider() {
        // $('.Mynavbar').toggleClass('safarifix');
        $('body').toggleClass('slided');
        $('aside').toggleClass('slidedside');
        $('.overlay').toggleClass('overlayOn');

      };

      $('.mobileAdd').click(function(event) {
        /* Act on the event */
        event.preventDefault();
        console.log("mobileAdd clicked")
        toggleSlider();
      });

      $('.overlay').click(function(event) {
        /* Act on the event */
        toggleSlider();
      });

      $('.icon-cancel').click(function(event) {
        /* Act on the event */
        toggleSlider();
      });


    });
  </script>


